    "rxjs": "5.0.0-rc.1",
    "serve-favicon": "~2.3.0",
    "stripe": "^4.14.0",
    "tinymce": "^4.5.1",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
